approve(spender,tokens):
Requires		msg.sender != address(0)
Ensures		decimals == VeriSol.Old(uint256(decimals))
Ensures		allowed[msg.sender][spender] == tokens
Ensures		VeriSol.Old(_totalSupply) == _totalSupply
transfer(to,tokens):
Requires		msg.sender != address(0)
Requires		VeriSol.Old(balances[msg.sender]) >= tokens
Ensures		VeriSol.Old(VeriSol.SumMapping(balances)) == VeriSol.SumMapping(balances)
Ensures		VeriSol.Old(balances[msg.sender]) >= balances[msg.sender]
Ensures		VeriSol.Old(balances[to]) <= balances[to]
Ensures		decimals == VeriSol.Old(uint256(decimals))
Ensures		VeriSol.Old(_totalSupply) == _totalSupply
Ensures		balances[to] >= tokens
transferFrom(from,to,tokens):
Requires		msg.sender != address(0)
Requires		VeriSol.Old(balances[from]) != VeriSol.Old(allowed[from][msg.sender])
Requires		VeriSol.Old(balances[from]) >= tokens
Requires		VeriSol.Old(balances[to]) != VeriSol.Old(allowed[from][msg.sender])
Requires		VeriSol.Old(allowed[from][msg.sender]) >= tokens
Ensures		VeriSol.Old(VeriSol.SumMapping(balances)) >= VeriSol.SumMapping(balances)
Ensures		VeriSol.Old(balances[from]) >= balances[from]
Ensures		VeriSol.Old(balances[to]) <= balances[to]
Ensures		allowed[from][msg.sender] <= VeriSol.Old(allowed[from][msg.sender])
Ensures		decimals == VeriSol.Old(uint256(decimals))
Ensures		VeriSol.Old(_totalSupply) == _totalSupply
