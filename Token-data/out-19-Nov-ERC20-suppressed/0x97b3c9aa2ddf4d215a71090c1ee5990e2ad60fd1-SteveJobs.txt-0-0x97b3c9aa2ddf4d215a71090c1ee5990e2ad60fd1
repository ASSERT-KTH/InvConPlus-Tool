approve(spender,tokens):
Requires		msg.sender != address(0)
Ensures		tokens == allowed[msg.sender][spender]
Ensures		_totalSupply == VeriSol.Old(_totalSupply)
Ensures		decimals == VeriSol.Old(uint256(decimals))
transferFrom(from,to,tokens):
Requires		msg.sender != address(0)
Requires		VeriSol.Old(balances[from]) >= tokens
Requires		tokens <= VeriSol.Old(allowed[from][msg.sender])
Requires		VeriSol.Old(allowed[from][msg.sender]) != VeriSol.Old(balances[to])
Ensures		VeriSol.Old(balances[from]) >= balances[from]
Ensures		tokens <= balances[to]
Ensures		_totalSupply == VeriSol.Old(_totalSupply)
Ensures		VeriSol.Old(allowed[from][msg.sender]) >= allowed[from][msg.sender]
Ensures		VeriSol.Old(VeriSol.SumMapping(balances)) >= VeriSol.SumMapping(balances)
Ensures		VeriSol.Old(balances[to]) <= balances[to]
Ensures		decimals == VeriSol.Old(uint256(decimals))
Ensures		allowed[from][msg.sender] + tokens == VeriSol.Old(allowed[from][msg.sender])
