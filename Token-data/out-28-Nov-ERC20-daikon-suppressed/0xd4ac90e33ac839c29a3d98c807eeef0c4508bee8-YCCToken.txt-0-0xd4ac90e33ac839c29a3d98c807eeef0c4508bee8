transfer(to,tokens):
Requires		decimals == _totalSupply
Requires		decimals == allowed[to][msg.sender]
Requires		_totalSupply == allowed[to][msg.sender]
Requires		decimals == 0
Requires		_totalSupply == 0
Requires		decimals < tokens
Requires		decimals < block.timestamp
Requires		decimals <= VeriSol.Old(VeriSol.SumMapping(balances))
Requires		decimals <= balances[to]
Requires		decimals < balances[msg.sender]
Requires		_totalSupply < tokens
Requires		_totalSupply < block.timestamp
Requires		_totalSupply <= VeriSol.Old(VeriSol.SumMapping(balances))
Requires		_totalSupply <= balances[to]
Requires		_totalSupply < balances[msg.sender]
Requires		tokens <= balances[msg.sender]
Requires		block.timestamp < balances[msg.sender]
Ensures		decimals == _totalSupply
Ensures		decimals == VeriSol.Old(decimals)
Ensures		decimals == VeriSol.Old(_totalSupply)
Ensures		decimals == VeriSol.Old(allowed[to][msg.sender])
Ensures		_totalSupply == VeriSol.Old(decimals)
Ensures		_totalSupply == VeriSol.Old(_totalSupply)
Ensures		_totalSupply == VeriSol.Old(allowed[to][msg.sender])
Ensures		to == VeriSol.Old(to)
Ensures		tokens == VeriSol.Old(tokens)
Ensures		msg.sender == VeriSol.Old(msg.sender)
Ensures		block.timestamp == VeriSol.Old(block.timestamp)
Ensures		decimals == 0
Ensures		_totalSupply == 0
Ensures		balances[msg.sender] >= 0
