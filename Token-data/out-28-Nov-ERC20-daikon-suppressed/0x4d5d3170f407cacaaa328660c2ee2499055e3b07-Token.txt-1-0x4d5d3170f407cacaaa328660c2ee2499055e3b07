Requires		totalSupply == decimals
Requires		totalSupply == allowance[_to][msg.sender]
Requires		decimals == allowance[_to][msg.sender]
Requires		totalSupply == 0
Requires		decimals == 0
Requires		totalSupply < _value
Requires		totalSupply < block.timestamp
Requires		totalSupply <= VeriSol.Old(VeriSol.SumMapping(balanceOf))
